 <% if store.liked_by?(current_user) %>
    <%= link_to store.likes.count, store_likes_path(store), method: :delete , remote: true, class: "fas fa-heart heart_red" %>
  <% else %>
    <%= link_to book.likes.count, store_likes_path(book), method: :post , remote: true, class: "fas fa-heart heart_blue" %>
  <% end %>


